net: "train_val.prototxt"
test_iter: 651
# make test net, but don't invoke it from the solver itself
test_interval: 1000000
display: 20
average_loss: 20
lr_policy: "fixed"
# lr for unnormalized softmax -- see train_val definition
base_lr: 1e-10
# high momentum
momentum: 0.99
# no gradient accumulation
iter_size: 1
max_iter: 80000
weight_decay: 0.0005
snapshot: 1000
snapshot_prefix: "snapshots_camvid300/train_lr1e-12_12300_lr1e-10_12500/"
test_initialization: false
